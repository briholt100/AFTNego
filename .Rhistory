###########################
ah$Div.Name<-"Admin"
ah$Div.Name[grep("stude|SDS", ah$Division, ignore.case=T, value=F)] <- "Student Services"
ah$Div.Name[grep("Humanities", ah$Division, ignore.case=T, value=F)] <- "AHSS"
ah$Div.Name[grep("Trans", ah$Division, ignore.case=T, value=F)] <- "BTS"
ah$Div.Name[grep("Business", ah$Division, ignore.case=T, value=F)] <- "BEIT"
ah$Div.Name[grep("health", ah$Division, ignore.case=T, value=F)] <- "HHS"
ah$Div.Name[grep("Math", ah$Division, ignore.case=T, value=F)] <- "MS"
ah$Div.Name[grep("Library", ah$Division, ignore.case=T, value=F)] <- "Lib.Coun"
ah$Emp.Stat<-"PT.Fac"
ah$Emp.Stat[grep("exempt", ah$Employ.Status, ignore.case=T, value=F)]<-"Exempt"
ah$Emp.Stat[grep("Part Time", ah$Employ.Status, ignore.case=F, value=F)]<-"PT.Fac"
ah$Emp.Stat[grep("Full Time", ah$Employ.Status, ignore.case=F, value=F)]<-"FT.Fac"
ah$Emp.Stat[grep("Professional Exempt", ah$Employ.Status, ignore.case=F, value=F)]<- "Pro.Exempt"
ah$Emp.Stat[grep("priority", ah$Employ.Status, ignore.case=T, value=F)]<-"PT.Priority"
ah$Emp.Stat[grep("counsel", ah$Employ.Status, ignore.case=T, value=F)]<-"FT.Fac"
ah$Emp.Stat[grep("Classified Staff", ah$Employ.Status, ignore.case=F, value=F)]<-"Class.Staff"
#################
#####
levels(ah$Advocate.Any.Honors)<-c(levels(ah$Advocate.Any.Honors),"IDK","Yes","No")
ah$Advocate.Any.Honors[grep("unsure",ah$Advocate.Any.Honors,ignore.case=T,value=F)]<-"IDK"
ah$Advocate.Any.Honors[ah$Advocate.Any.Honors=="yes"]<-"Yes"
ah$Advocate.Any.Honors[ah$Advocate.Any.Honors=="no"]<-"No"
ah$Advocate.Any.Honors<-factor(ah$Advocate.Any.Honors)
###########################
#combine employee types
###########################
ah$Employee.class<-"Classified"
ah$Employee.class[grep("time",ah$Employ.Status,ignore.case=T,value=F)]<-"Faculty"
ah$Employee.class[grep("exempt",ah$Employ.Status,ignore.case=T,value=F)]<-"Admin"
ah$Employee.class[grep("professional",ah$Employ.Status,ignore.case=T,value=F)]<-"Prof"
table(ah$Employee.class)
table(ah$Employee.class,ah$Div.Name)
###########################
#combine preferences
###########################
ah$Pref<-"misc"
ah$Pref[grep("t support any Honors",ah$Support.Honors.Form,value=F)]<-"No.Honors"
ah$Pref[grep("administered by North employees",ah$Support.Honors.Form,value=F)]<-"NorthHonors"
ah$Pref[grep("administered by North employees | the American",ah$Support.Honors.Form,value=F)]<-"EitherHonors"
ah$Pref[grep("know|preference",ah$Support.Honors.Form,value=F)]<- "do.not.Care.or.Know"
ah$Pref[grep("know",ah$Support.Honors.Form,value=F)]<-"do.not.Know"
ah$Pref[ah$Pref == "EitherHonors" & ah$Support.Honors.Form
=="a partnership with the American Honors company who would create and administer the program"]<-"AmerHonors"
table(ah$Pref)
